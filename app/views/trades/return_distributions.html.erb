            <th style="width:50%">Count</th>
          </tr>
          </thead>
          <tbody>
            <% for @return_distribution in @return_distributions %>
              <tr>
                <td><%= @return_distribution[0] %></td>
                <td><%= @return_distribution[1] %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
<% end %>
