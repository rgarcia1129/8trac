<% if @instrument %>


    <div class="widget-12 panel no-border widget-loader-circle no-margin">
      <div class="panel-body" >
        <div class="row">
          <div class="col-xlg-8 ">
            <div class="p-l-10">
              <h2 class="pull-left"><%= @instrument.name %> by <%= @instrument.symbol%> (<%= number_to_percentage(@instrument.open_price, :precision => 0) %>)</h2>
              <br>
              <div style="width:98%">
                <div class="progress">
                    <div class="progress-bar progress-bar-complete" style="width:<%= @instrument.open_price%>%"></div>
                </div>
              </div>
            </div>
            </div>
            </div>
            <div class="row">
              <div class="col-md-6 ">
                <%=image_tag(@instrument.fundamentals_url)%>
            </div>
            <div class="col-md-6">
              <div style="color:green"><h2><%= number_to_currency(10000 * (@instrument.open_price / 100)) %> of <%= number_to_currency(10000)%></h2></div>
              <% if @instrument.status == 'Funding in Progress' %>
                <%= distance_of_time_in_words DateTime.yesterday, DateTime.now.in_time_zone rescue nil %> Remaining <br><br>
              <% end %>
              <b>Status </b>
              <%= @instrument.status %><br><br>
              <b>Description</b><br>
              <%= @instrument.description %>

              <div  style='text-align:center' width=100%>
                <br>
                <br>
                <button class="btn btn-success btn-cons" data-toggle="modal" data-target="#myModal">
                  <% if @instrument.status == 'Released' %>

                    Buy
                  <% elsif @instrument.status == 'Recording' or @instrument.status == 'Mastering'  %>
                    Pre Order
                  <% else%>
                    Contribute
                  <% end %>
                </button>
              </div>
            
            </div>
          </div>
        </div>



